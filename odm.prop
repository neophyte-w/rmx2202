# Boot & Kernel
ro.build.kernel.id=5.4.254
ro.oplus.minidump.kernel.log.support=true
persist.sys.assert.panic=false
persist.sys.assert.state=false
persist.sys.dump=0
persist.sys.powerkeydump=0

# Display
vendor.display.primary_mixer_stages=9
ro.oplus.screenhole.border=8

# Touch & Sensor
persist.sys.oppo.proximity=true
persist.sys.oppo.fusionlight=true

# Debug & Compatibility
persist.sys.enable_slpi_dump=1
ro.oplus.ffmpeg.enable=true
ro.oplus.audio.support.mss=1
